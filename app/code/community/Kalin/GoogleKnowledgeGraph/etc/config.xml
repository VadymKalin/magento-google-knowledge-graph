<config>
    <modules>
        <Kalin_GoogleKnowledgeGraph>
            <version>0.0.1</version>
        </Kalin_GoogleKnowledgeGraph>
    </modules>
    <global>
        <models>
            <gogleknowledgegraph>
                <class>Kalin_GoogleKnowledgeGraph_Model</class>
            </gogleknowledgegraph>
        </models>
        <blocks>
            <googleknowledgegraph>
                <class>Kalin_GoogleKnowledgeGraph_Block</class>
            </googleknowledgegraph>
        </blocks>
        <helpers>
            <googleknowledgegraph>
                <class>Kalin_GoogleKnowledgeGraph_Helper</class>
            </googleknowledgegraph>
        </helpers>
        <resources>
            <googleknowledgegraph_setup>
                <module>Kalin_GoogleKnowledgeGraph</module>
                <class>Mage_Core_Resource_Model_Abstract</class>
            </googleknowledgegraph_setup>
        </resources>
    </global>
    <frontend>
        <translate>
            <modules>
                <Kalin_GoogleKnowledgeGraph>
                    <files>
                        <default>Kalin_GoogleKnwledgeGraph.csv</default>
                    </files>
                </Kalin_GoogleKnowledgeGraph>
            </modules>
        </translate>
        <layout>
            <updates>
                <googleknowledgegraph>
                    <file>googleknowledgegraph.xml</file>
                </googleknowledgegraph>
            </updates>
        </layout>
    </frontend>
    <adminhtml>
        <translate>
            <modules>
                <Kalin_GoogleKnowledgeGraph>
                    <files>
                        <default>Kalin_GoogleKnowledgeGraph.csv</default>
                    </files>
                </Kalin_GoogleKnowledgeGraph>
            </modules>
        </translate>
    </adminhtml>
</config>