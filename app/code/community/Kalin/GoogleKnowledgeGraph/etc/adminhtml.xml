<config>
    <acl>
        <resources>
            <admin>
                <children>
                    <system>
                        <children>
                            <config>
                                <children>
                                    <googleknowledgegraph translate="title" module="googleknowledgegraph">
                                        <title>Google Knowledge Graph</title>
                                    </googleknowledgegraph>
                                </children>
                            </config>
                        </children>
                    </system>
                </children>
            </admin>
        </resources>
    </acl>
</config>